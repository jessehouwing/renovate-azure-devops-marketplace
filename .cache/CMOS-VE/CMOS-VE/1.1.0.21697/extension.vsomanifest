{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "CMOS-VE",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "CMOS-VE",
                "uri": "CMOS-VE.html",
                "icon": "img/CMOS-VE-icon.png"
            }
        },
        {
            "id": "VEScanner",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/VEScanner"
            }
        },
        {
            "id": "VEScanner-results-tab",
            "type": "ms.vss-build-web.build-results-tab",
            "description": "A tab contributing to build results view and depends on Cmdline task",
            "targets": [
                "ms.vss-build-web.build-results-view"
            ],
            "properties": {
                "name": "VE Scanner Report",
                "uri": "results-tab.html"
            }
        }
    ],
    "scopes": [
        "vso.code",
        "vso.work",
        "vso.build_execute"
    ],
    "contributionTypes": []
}