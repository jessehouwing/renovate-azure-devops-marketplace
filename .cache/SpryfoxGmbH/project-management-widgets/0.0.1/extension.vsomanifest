{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/spryfox-analytics/azure-devops-project-management-widgets"
    },
    "demands": [
        "contribution/ms.vss-dashboards-web.widget-sdk-version-2"
    ],
