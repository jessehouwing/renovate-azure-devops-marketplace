{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "work-item-copilot-hub",
            "type": "ms.vss-web.hub",
            "description": "An Azure DevOps extension for work item copilot hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Work Item Co-pilot",
                "uri": "dist/WorkItemCopilot/index.html",
                "icon": {
                    "light": "static/logos/logo_small.png",
                    "dark": "static/logo_small.png"
                }
            }
        },
        {
            "id": "work-item-copilot-form-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "An Azure DevOps extension for work item copilot form group",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Work Item Co-pilot",
                "uri": "dist/WorkItemCopilotFormGroup/index.html",
                "height": 450
            }
        },
        {
            "id": "user-story-breakout-copilot-form-page",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "An Azure DevOps extension for user story breakout copilot form page",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Break into User Stories",
                "uri": "dist/WorkItemCopilotFormPage/index.html"
            }
        }
    ],
    "scopes": [
        "vso.project",
        "vso.work",
        "vso.work_write"
    ],
    "baseUri": "https://salmon-sea-0897bfa1e.3.azurestaticapps.net",
    "contributionTypes": []
}