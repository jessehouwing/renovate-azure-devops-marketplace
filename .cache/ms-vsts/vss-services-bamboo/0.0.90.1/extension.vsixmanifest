<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="vss-services-bamboo" Version="0.0.90.1" Publisher="ms-vsts"/>
    <DisplayName>Atlassian Bamboo Integration</DisplayName>
    <Description xml:space="preserve">Bamboo is a continuous integration and delivery server from Atlassian.</Description>
    <ReleaseNotes>Bamboo is a continuous integration and delivery tool that ties automated builds, tests and releases together in a single workflow. With this integration you will be able to trigger builds after code commits and deployments after a build has completed.</ReleaseNotes>
    <GalleryFlags>Public MultiVersion</GalleryFlags>
    <Categories>Build and release</Categories>
    <LongDescription>Bamboo is a continuous integration and delivery tool that ties automated builds, tests and releases together in a single workflow. With this integration you will be able to trigger builds after code commits and deployments after a build has completed.</LongDescription>
    <Icon>icons/bamboo-logo.png</Icon>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#205081"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
    </Properties>
  </Metadata>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Integration"/>
  </Installation>
  <Dependencies/>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="details.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="icons/bamboo-logo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Branding" d:Source="File" Path="icons/bamboo-refresh.png" Addressable="true"/>
    <Asset Type="images/temp.png" d:Source="File" Path="images/temp.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" Path="extension.vsomanifest" Addressable="true" d:Source="File"/>
  </Assets>
