<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="services-hipchat" Version="1.0.3" Publisher="ms-vsts"/>
    <DisplayName>HipChat Integration</DisplayName>
    <Description xml:space="preserve">Group, private chat, and file sharing that is fully integrated with your development lifecycle.</Description>
    <ReleaseNotes>Great teams use HipChat. With this integration you will be able to send work item, version control, and build artifact events to a HipChat room to maximize visibility and collaboration.</ReleaseNotes>
    <Tags>atlassian,chat,im</Tags>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Collaborate</Categories>
    <LongDescription>Great teams use HipChat. With this integration you will be able to send work item, version control, and build artifact events to a HipChat room to maximize visibility and collaboration.</LongDescription>
    <Icon>icons/hipchat.png</Icon>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://www.visualstudio.com/get-started/hipchat-and-vso-vs"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="http://help.hipchat.com/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Learn" Value="https://www.hipchat.com/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Pricing" Value="https://www.hipchat.com/pricing"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#205081"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
    </Properties>
  </Metadata>
  <Installation>
    <InstallationTarget Id="Microsoft.TeamFoundation.Server.Integration" Version="[12.0-15)"/>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Integration"/>
  </Installation>
  <Dependencies/>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="details.md" Addressable="true"/>
