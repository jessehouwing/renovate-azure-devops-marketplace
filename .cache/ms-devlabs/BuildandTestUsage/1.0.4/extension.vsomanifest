{
    "manifestVersion": 1,
    "scopes": [
        "vso.build"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "mp_terms.md"
        }
    },
    "contributions": [
        {
            "id": "Widget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Build Usage",
                "description": "See the build usage for your builds in this Team Project",
                "previewImageUrl": "img/preview.png",
                "uri": "build-usage-widget.html",
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 1
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "BuildHub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-hub-group"
            ],
            "properties": {
                "name": "Build Usage",
                "order": 99,
                "uri": "build-usage-hub.html"
            }
        },
        {
            "id": "BuildAccountHomeHubGroup",
            "type": "ms.vss-web.hub",
            "description": "Adds a hub to the Account hub group",
            "targets": [
                "ms.vss-web.collection-home-hub-group"
            ],
            "properties": {
                "name": "Build Usage",
                "order": 100,
                "uri": "build-usage-group-hub.html"
            }
        }
    ],
    "contributionTypes": []
}