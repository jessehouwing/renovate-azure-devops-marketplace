{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "url": "https://github.com/llima/code-management-extension"
    },
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "code-management-feature-branch-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "description": "Control for creating the feature brach",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Codenames",
                "uri": "dist/codenames/codenames.html",
                "height": 36,
                "inputs": [
                    {
                        "id": "RepoFieldName",
                        "name": "Field with repo name",
                        "description": "Reference repo name field to be updated.",
                        "type": "WorkItemField",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    }
                ]
            }
        }
    ],
    "contributionTypes": []
}