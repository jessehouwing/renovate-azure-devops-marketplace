{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "TestCoverage",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                "arkv.test-coverage-dashboard-widget.TestCoverage.Configuration"
            ],
            "properties": {
                "name": "Test Coverage Widget",
                "description": "Dashboard listing all requirements and showing their test coverage percentage.",
                "catalogIconUrl": "img/logo.png",
                "previewImageUrl": "img/logo.png",
                "uri": "src/widget/widget.html",
                "supportedSizes": [
                    {
                        "rowSpan": 8,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 10,
                        "columnSpan": 9
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "TestCoverage.Configuration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [
                "ms.vss-dashboards-web.widget-configuration"
            ],
            "properties": {
                "name": "Test Coverage (Configuration)",
                "description": "Configures the Test Coverage Widget",
                "uri": "configuration.html"
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.test"
    ],
    "contributionTypes": []
}