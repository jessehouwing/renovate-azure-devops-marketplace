{
    "manifestVersion": 1,
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "RESTRequestButton",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "REST Request Button",
                "uri": "dist/RESTRequestButton/RESTRequestButton.html",
                "inputs": [
                    {
                        "id": "ButtonTitle",
                        "name": "Text you want displayed on the button",
                        "description": "Text you want displayed on the button",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "Fields",
                        "name": "Fields of the work item you want to collect (comma separated)",
                        "description": "Work item fields you wish to collect for sending, comma-separated",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "Endpoint",
                        "name": "URL you want to send your data to",
                        "description": "URL you want to send your data to",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "RequestConfig",
                        "name": "options json",
                        "description": "fetch optnions in JSON format, see https://developer.mozilla.org/en-US/docs/Web/API/fetch#parameters for more information",
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    }
                ]
            }
        }
    ],
    "contributionTypes": []
}