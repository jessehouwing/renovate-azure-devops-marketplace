{
    "manifestVersion": 1,
    "demands": [
        "api-version/3.0"
    ],
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "EapWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                "portfolio-project.EapWidget.Configuration",
                "portfolio-project-dev.EapWidget.Configuration"
            ],
            "properties": {
                "name": "EAP Widget",
                "description": "EAP Widget for your project",
                "catalogIconUrl": "build/image/logo-rodrigocipriani.png",
                "previewImageUrl": "build/image/logo-rodrigocipriani.png",
                "uri": "#/widget/eap",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 6,
                        "columnSpan": 6
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "hello-world-hub-group",
            "type": "ms.vss-web.hub-group",
            "description": "Sample hub group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Portfolio Project",
                "icon": {
                    "dark": "build/image/logo-rodrigocipriani.png",
                    "light": "build/image/logo-rodrigocipriani.png"
                }
            }
        },
        {
            "id": "default-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                ".hello-world-hub-group"
            ],
            "properties": {
                "uri": "#/hub/hub"
            }
        }
    ],
    "contributionTypes": []
}