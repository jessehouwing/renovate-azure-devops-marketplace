    <GalleryFlags>Public</GalleryFlags>
    <Categories>Azure Repos</Categories>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/daniecas/azure-devops-mermaid-viewer"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#usage"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Learn" Value="https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#run-locally"/>
