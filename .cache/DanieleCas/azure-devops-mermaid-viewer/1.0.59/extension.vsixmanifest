    <Asset Type="doc/preview_diagram.png" d:Source="File" Path="doc/preview_diagram.png" Addressable="true"/>
    <Asset Type="doc/pr_preview_diagram.png" d:Source="File" Path="doc/pr_preview_diagram.png" Addressable="true"/>
    <Asset Type="doc/text_diagram.png" d:Source="File" Path="doc/text_diagram.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.License" d:Source="File" Path="LICENSE" Addressable="true"/>
