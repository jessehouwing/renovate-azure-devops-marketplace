      <Property Id="Microsoft.VisualStudio.Services.Links.Issues" Value="https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#for-contributors"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Privacypolicy" Value="https://github.com/daniecas/azure-devops-mermaid-viewer/blob/main/Privacy.md"/>
    </Properties>
    <Icon>doc/mermaid.png</Icon>
    <License>LICENSE</License>
  </Metadata>
  <Dependencies/>
