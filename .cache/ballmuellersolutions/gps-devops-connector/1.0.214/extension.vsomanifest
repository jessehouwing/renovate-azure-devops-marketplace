{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "capamap.settings",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "CAPAMAP Settings",
                "uri": "dist/settings/index.html",
                "icon": "asset://static/sample-icon.png",
                "supportsMobile": true
            }
        },
        {
            "id": "Capamap.TypePicker",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "CAPAMAP Type picker",
                "uri": "dist/type-picker/index.html",
                "height": 600,
                "supportsMobile": true,
                "inputs": [
                    {
                        "id": "Capamap.TypePicker.Field",
                        "description": "The field associated with the control.",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "String"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    }
                ]
            }
        }
    ],
    "scopes": [],
    "contributionTypes": []
}